{
  "name": "pixiv-telegram-bot",
  "version": "0.0.1",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "serve": "npx nodemon -e js,ts --watch src --exec \"yarn build && yarn start\"",
    "build": "tsc --build tsconfig.json",
    "start": "node built/index",
    "lint": "npx eslint . && npx prettier -c .",
    "lint:fix": "npx eslint --fix . && npx prettier -w ."
  },
  "repository": {
    "url": "https://github.com/sleroq/pixiv-telegram-bot"
  },
  "exports": "./built/index.js",
  "type": "module",
  "licence": "GPL-2.0-or-later",
  "keywords": [
    "pixiv",
    "telegram",
    "bot"
  ],
  "dependencies": {
    "@grammyjs/parse-mode": "^1.1.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "grammy": "^1.8.3",
    "pino": "^8.0.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  }
}
